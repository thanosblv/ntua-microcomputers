IN 10H

MVI H,09H
MVI L,00H
MVI A,FFH
MVI D,00H
MVI E,08H

LIST_LOOP:    CPI 00H
		JZ LIST_END
		CALL ZEROS_OF_A
		DCR A
		JMP LIST_LOOP

ZEROS_OF_A:  PUSH PSW
		MOV B,A
		PUSH B
		MVI C,00H
		MOV A,C

	LOOP2: CPI 08H
		JZ END_LOOP2
		MOV A,B
		RRC
		JNC D_INCREASE
      D_BACK: MOV B,A
		INR C
		MOV A,C
		JMP LOOP2
   
  D_INCREASE: INX D
		JMP D_BACK

   END_LOOP2: POP PSW
		POP B
		MOV A,B
		RET

LIST_END: 

		RST 1
		END	