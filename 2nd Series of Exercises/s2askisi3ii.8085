MVI B,10H

READ_INPUT:	CALL KIND
		MVI C,00H
		CPI 05H
		JC FIX_LEDS_1
		CPI 09H
		JNC READ_INPUT
		JMP FIX_LEDS_2

FIX_LEDS_1:	MVI D,F0H
		JMP LOOP1

FIX_LEDS_2:	MVI D,00H
		JMP LOOP1

LOOP1:	MOV A,C
		CPI 04H
		JZ READ_INPUT
		MOV A,D
		STA 3000H
		CALL DELB
		MVI A,FFH
		STA 3000H
		CALL DELB
		INR C
		JMP LOOP1

END